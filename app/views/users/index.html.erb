<h1><%= t :listofusers %></h1>

<table>
  <thead>
    <tr>
		<th><%= t :login %></th>
      <th><%= t :email %></th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.login %></td>
        <td><%= user.email %></td>
        <td><%= link_to t('show'), user %></td>
        <td><%= link_to t('edit'), edit_user_path(user) %></td>
        <td><%= link_to t('destroy'), user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<% file = File.open('log/development.log', 'r') do |f| %>
	<% f.each do |line| %>
		<%= line %>
		<br>
	<% end %>
<% end %>
